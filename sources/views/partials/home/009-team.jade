section.team#team
  .container
    h3.team__title.title-line Our team
      span.title-line__line(data--20-bottom="width: 0px" data--200-bottom="width: 118px")
    .row.team__row
      each status, name in team
        .person
          .person__photo
            .person__photo-wrap
              img.person__photo-img(src="img/home-page/team-"+ count++ +".jpg", alt="team-person-" +count)
          h5.person__name=name
          p.person__status=status
          a(href=teamLink[count], target="_blank", rel="nofollow").person__link
      -count = 1

    //p.team__advisors-title Advisors
