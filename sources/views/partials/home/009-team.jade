          p.person__status=status
          a(href="#", target="_blank", rel="nofollow").person__link
      -count = 1
    p.team__partner-title Partners
    .row.team__row
      each alt, index in partners
        .partner
          img.partner__logo(src="img/home-page/partners-"+ index +".png", alt=alt)
